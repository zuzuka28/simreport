{
  "name": "frontend",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --ignore-path .gitignore -u --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "generateApi": "npx openapi-typescript ../simrep/api/rest/doc/openapi.yaml --output src/providers/simrep/schema.ts"
  },
  "version": "0.1.0",
  "description": "A Web App created by create-awesome-node-app",
  "engines": {
    "node": ">=18.15"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "openapi-fetch": "^0.13.0",
    "openapi-react-query": "^0.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.80.6",
    "typescript": "^4.9.5",
    "vite": "^4.5.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.14.7"
  }
}
